server {
      listen 80;
      listen [::]:80;

      server_name 172.17.0.2;

      root /usr/src/app/dist;
      index index.html;

      location / {
              try_files $uri /index.html =404;
              add_header Cache-Control 'must-revalidate, proxy-revalidate, max-age=86400';
      }
}
