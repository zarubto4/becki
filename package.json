    "start-https": "webpack-dev-server --inline --progress --port 8181 --https",
    "lint": "tslint --force \"src/**/*.ts\"",
    "docs": "typedoc --options typedoc.json src/app/main.ts",
    "start": "npm run server",
    "dev": "webpack --config ./webpack.config.dev.js ./src/main.ts --mode development",
    "prod": "webpack --config ./webpack.config.prod.js  --mode production",
    "build-dev": "webpack --mode development",
    "build-prod": "webpack --mode production",
    "check": "webpack --config webpack.config.dev.js ./src/main.ts  --display-error-details"
